MessageDerivedValues = [
    messageId: MessageId,
    hubAcceptedTimestamp: Timestamp,
    mlsGroupId: bstr,
    senderLeafIndex: uint .size 4,
    senderClientUrl: MsgUri,
    senderUserUrl: MsgUri,
    roomUrl: MsgUri
]

MsgUri = tstr
MessageId = bstr .size 32
Timestamp = MsecsSinceEpoch / TaggedTime
MsecsSinceEpoch = uint .size 8  ; milliseconds since start of UNIX epoch
TaggedTime = #6.62({* name => value })
name = int / tstr .size (1..255)
value = any .size (0..4095)
